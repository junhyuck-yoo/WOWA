<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pjt.wowa.mappers.memberMapper">
	
	<select id="login" parameterType="com.pjt.wowa.MemberVO" resultType="com.pjt.wowa.MemberVO">
		SELECT
		* 
		FROM member 
		WHERE id = #{id}
		AND password = #{password}
	</select>




</mapper>
